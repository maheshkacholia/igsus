O1 := OPEN[-1]
O2 := OPEN[-2]
H1 := HIGH[-1]
H2 := HIGH[-2]
L1 := LOW[-1]
L2 := LOW[-1]
C1 := CLOSE[-1]
C2 := CLOSE[-2]
O  := OPEN[0]
C  := CLOSE[0]
H  := HIGH[0]
