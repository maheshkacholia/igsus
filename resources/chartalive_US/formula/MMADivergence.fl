 A := ( CLOSE[0] > MMA[8;0] ) AND ( CLOSE[-1] > MMA[8;-1] )
B :=   ( CLOSE[-2] < MMA[8;-2]  ) 
BUY1 := A AND B
IF ( BUY1 )
{
    BUYTYPE := "STRONGBUY"
    BUY{ "" }
}
C := ( CLOSE[0] < MMA[8;0] ) AND ( CLOSE[-1] < MMA[8;-1] )
